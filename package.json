{
  "name": "@baleada/vue-heroicons",
  "version": "0.0.2",
  "description": "Vue components that wrap up the Heroicons set for the Baleada toolkit",
  "type": "module",
  "module": "lib/index.js",
  "exports": {
    "import": "./lib/index.js"
  },
  "scripts": {
    "prepare": "rollup --config rollup.config.js",
    "test:only": "run() { npx uvu tests/$2 $1.test.js$; }; run",
    "test": "npm run test:only .",
    "dev": "vite",
    "update-icons": "cd git_modules/heroicons && git pull"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baleada/vue-heroicons.git"
  },
  "keywords": [
    "baleada",
    "icons",
    "vue",
    "heroicons"
  ],
  "author": {
    "name": "Alex Vipond",
    "email": "hello@alexvipond.dev",
    "url": "https://alexvipond.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baleada/vue-heroicons/issues"
  },
  "homepage": "https://baleada.netlify.com",
  "devDependencies": {
    "@baleada/prepare": "^0.4.29",
    "@vue/compiler-sfc": "^3.0.4",
    "rollup": "^2.35.1",
    "uvu": "^0.5.1",
    "vite": "^1.0.0-rc.13"
  },
  "sideEffects": false
}
